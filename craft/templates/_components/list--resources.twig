{# Resources List #}
<div class="list-accordion list--resources md:px-0 px-6  sm:pt-10 sm:mb-10  {{classes}} ">
	
	{# Section Header #}
	{% include '_components/headers' with {
			type: 'section-header--divider', 
			'title': block.sectionTitle
		} 
	%}

	{% set list = block.listLinkDocContact %}
	{% for block in list %}
		{% switch block.type %}
		
			{% case "linkDownload" %}
				{% include '_components/list--resources__download' %}

			{% case "info" %}
				{% include '_components/list--resources__accordion' %}

			{% case "contact" %}
				{% include '_components/list--resources__contact' %}
		{% endswitch %}
		
	{% endfor %}

</div>

